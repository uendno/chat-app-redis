<!DOCTYPE html>
<html>
<head>
    <!--Import Google Icon Font-->
    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="bower_components/materialize/bin/materialize.css"
          media="screen,projection"/>
    <link type="text/css" rel="stylesheet" href="bower_components/bootstrap/dist/css/bootstrap.min.css">
    <link type="text/css" rel="stylesheet" href="css/style.min.css">
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
</head>

<body>

<div class="container">

    <div class="row wrapper">

        <div class="col s12 m6 center">
            <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                    <span class="card-title">Title</span>
                    <p>Sub title etc</p>
                </div>
                <div class="card-action white-text">
                    <div class="col s6 m3 offset-m3">
                        I am <br/><br/>
                    </div>

                    <div class="col s6 m3">
                        <select class="form-control" id="my-gender">
                            <option value="female">Female</option>
                            <option value="male">Male</option>
                        </select>
                    </div>
                    <br/><br/>

                    <div class="col s6 m3 offset-m3">
                        I'm looking for <br/><br/>
                    </div>

                    <div class="col s6 m3">
                        <select class="form-control" id="pref-gender">
                            <option value="female">Female</option>
                            <option value="male">Male</option>
                        </select>
                    </div>
                    <br/><br/>

                    <div class="col s6 m3 offset-m3">
                        I'm <br/><br/>
                    </div>

                    <div class="col s6 m3">
                        <select class="form-control" id="my-age">
                            <% for(var i = 13; i < 100; i++) {
                                var selected = "";
                                if (i == 18) {
                                    selected = 'selected="true"';
                                }
                            %>
                            <option value="<%= i %>" <%= selected %>><%= i %> y.o.</option>
                            <% } %>
                        </select>
                    </div>
                    <br/><br/>

                    <div class="col s6 m3 offset-m3">
                        Pref. age <br/><br/>
                    </div>

                    <div class="col s6 m3">
                        <select class="form-control" id="pref-age">
                            <% ageRanges.forEach(function (range) { %>
                            <option><%= range.min %> - <%= range.max %></option>
                            <% }) %>
                        </select>
                    </div>
                    <br/><br/>

                    <br/>
                    <a class="waves-effect waves-light btn-large" id="btn-start" href="#">Find!</a> <br/><br/>

                    Cookies etc. <a href="regulations.html">Show more</a>

                </div>
            </div>
        </div>

        <div class="col s12 m6 center" id="chat-box">

            <div class="row">
                <ul id="messages">
                </ul>
            </div>
            <div class="row">
                <form action="" id="input-form">
                    <div class="row input-group">
                        <div class="input-field col s12">
                            <input placeholder="Enter your message..." id="input-message" type="text" class="validate"
                                   autocomplete="off">
                        </div>
                        <span class="input-group-btn">
                            <a class="waves-effect waves-light btn" id="send-button">Send</a>
                        </span>
                    </div>
                </form>
            </div>
        </div>
    </div>


</div>


<script type="text/javascript" src="bower_components/socket.io-client/dist/socket.io.min.js"></script>
<script type="text/javascript" src="bower_components/jquery/dist/jquery.min.js"></script>
<script type="text/javascript" src="bower_components/materialize/bin/materialize.js"></script>
<script type="text/javascript" src="bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
<script type="text/javascript" src="bower_components/xss/dist/xss.min.js"></script>
<script type="text/javascript" src="js/index.min.js"></script>
</body>
</html>